unit Base.BuilderSQL.Delete;

interface

uses
  Base.Utils,
  Base.BuilderSQL, System.SysUtils;

type
  TDeleteSQL = class( TBuilderSQL )
    function GetSQL(): string; override;
    class function Builder(): TBuilderSQL;
  end;



implementation

{ TDeleteSQL }

class function TDeleteSQL.Builder: TBuilderSQL;
begin
  raise Exception.Create(' TDeleteSQL.Builder não implemetado');
end;

function TDeleteSQL.GetSQL: string;
begin
  raise Exception.Create(' TDeleteSQL.GetSQL ');

end;

end.
